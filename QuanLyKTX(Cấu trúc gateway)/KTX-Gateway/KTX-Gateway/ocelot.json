{
  "Routes": [
    {
      "UpstreamPathTemplate": "/admin/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [ { "Host": "localhost", "Port": 8001 } ]
    },
    {
      "UpstreamPathTemplate": "/user/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [ { "Host": "localhost", "Port": 8002 } ]
    },
    {
      "UpstreamPathTemplate": "/auth/{everything}",
      "UpstreamHttpMethod": [ "POST", "OPTIONS" ],
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [ { "Host": "localhost", "Port": 8001 } ]
    }
  ],
  "GlobalConfiguration": { "BaseUrl": "http://localhost:8000" },
  "SwaggerEndPoints": [
    {
      "Key": "admin",
      "Config": [
        {
          "Name": "Admin API",
          "Version": "v1",
          "Url": "http://localhost:8001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "user",
      "Config": [
        {
          "Name": "User API",
          "Version": "v1",
          "Url": "http://localhost:8002/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}


